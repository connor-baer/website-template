{
  "version": 2,
  "name": "website-template",
  "regions": ["sfo", "bru", "gru"],
  "routes": [
    {
      "src": "/api/(.+)",
      "dest": "server/api/$1/index.js"
    }
  ],
  "builds": [
    { "src": "server/api/**/index.js", "use": "@now/node" },
    { "src": "package.json", "use": "@now/next@canary" }
  ]
}
